<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="@tools:sample/first_names">

    <Button
        android:id="@+id/button11"
        android:layout_width="123dp"
        android:layout_height="58dp"
        android:layout_marginTop="13dp"
        android:layout_marginBottom="28dp"
        android:backgroundTint="#ed8074"
        android:onClick="onClickIngresoProd"
        android:text="Ingresar Producto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.809"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listProducto" />

    <Button
        android:id="@+id/button12"
        android:layout_width="123dp"
        android:layout_height="58dp"
        android:layout_marginBottom="28dp"
        android:backgroundTint="#ed8074"
        android:onClick="onClickBackToMain"
        android:text="Regresar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listProducto"
        app:layout_constraintVertical_bias="1.0" />

    <ListView
        android:id="@+id/listProducto"
        android:layout_width="409dp"
        android:layout_height="632dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>